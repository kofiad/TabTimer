{
	"manifest_version": 3,
	"name": "Tab Timer",
	"version": "1.0.0",
	"description": "An extension for managing your tabs",
	"action": {
	  "default_popup": "index.html",
	  "default_icon": "icon_extension.png"
	},
	"permissions": [
		"tabs"
	],
	"background": {
		"service_worker": "background.js"
	}
}
